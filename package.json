{
  "name": "@ableron/express",
  "version": "1.0.0",
  "description": "Express Middleware for Ableron Server Side UI Composition",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=19"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prettier": "prettier . --write",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --no-cache",
    "test:cov": "NODE_OPTIONS=--experimental-vm-modules npx jest --coverage --no-cache --silent"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ableron",
    "express",
    "ssi",
    "server-side-includes",
    "ui-composition",
    "dynamic-ui-composition"
  ],
  "author": "Torsten Blasche <dev@t-blasche.de>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/ableron/ableron-express",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ableron/ableron-express.git"
  },
  "bugs": {
    "url": "https://github.com/ableron/ableron-express/issues"
  },
  "dependencies": {
    "@ableron/ableron": "1.0.3",
    "express-interceptor": "1.2.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "29.5.8",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "express": "4.18.2",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
