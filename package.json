  ],
  "author": "Torsten Blasche <dev@t-blasche.de>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/ableron/ableron-express",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ableron/ableron-express.git"
  },
  "bugs": {
    "url": "https://github.com/ableron/ableron-express/issues"
  },
  "dependencies": {
    "@ableron/ableron": "1.1.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "express": "4.18.2",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "supertest": "6.3.3",
    "ts-loader": "9.5.1",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
